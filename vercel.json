{
  "rewrites": [
    {
      "source": "/blog",
      "destination": "https://robeen-blog.vercel.app/blog"
    },
    {
      "source": "/blog/:path*",
      "destination": "https://robeen-blog.vercel.app/blog/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/support",
      "destination": "https://api.robeen.ai/support",
      "permanent": false
    }
  ]
}
